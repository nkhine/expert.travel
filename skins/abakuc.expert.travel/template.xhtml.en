<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
  "http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
<html xmlns="http://www.w3.org/1999/xhtml" 
  xmlns:stl="http://xml.itools.org/namespaces/stl">
  <head>
    <title>${title}</title>
    <!-- Meta -->
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
    <meta stl:repeat="meta meta_tags" name="${meta/name}"
          content="${meta/content}" />

    <!-- CSS -->
    <link href="fonts-min.css" rel="stylesheet" type="text/css" />
    <link href="grids-min.css" rel="stylesheet" type="text/css" />
    <!-- JavaScript -->
    <script type="text/javascript" stl:repeat="script scripts" src="${script}" 
      />
</head>
<body>
    <!-- choose fluid page and Template Preset -->
    <div id="doc3" class="yui-t4">
        <!-- main content block -->
        <div id="yui-main">
            <div class="yui-b">
                <!-- stacked grid structure, Special Grid "b" -->
                <div class="yui-gb">
                    <div class="yui-u first">Special Grid "b" First</div>
                    <div class="yui-u">yiu-u</div>
                    <div class="yui-u">yui-u</div>
                </div>
                <!-- stacked grid structure, Special Grid "c" -->
                <div class="yui-gc">
                    <div class="yui-u first">Special Grid "c" First</div>
                    <div class="yui-u">Special Grid "c"</div>
                </div>
                <!-- stacked grid structure -->
                <div class="yui-g">
                    <div class="yui-u first">stacked grid structure - first</div>
                    <div class="yui-u">stacked grid structure</div>
                </div>
            </div>
        </div>
        <!-- secondary content block -->
        <div class="yui-b">secondary content block</div>
    </div>
</body>
