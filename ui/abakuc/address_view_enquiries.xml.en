<?xml version="1.0" encoding="utf-8"?>
<stl:block xmlns="http://www.w3.org/1999/xhtml" 
  xmlns:stl="http://xml.itools.org/namespaces/stl">

<div stl:repeat="enquiry enquiries">
  <h5>${enquiry/date} (${enquiry/type})</h5>
  <p>
  From: ${enquiry/firstname} ${enquiry/lastname}, ${enquiry/email}
  <stl:block if="enquiry/phone"><br/>Phone: ${enquiry/phone}</stl:block>
  </p>
  <p>
  <a href=";view_enquiry?index=${enquiry/index}">View</a>
  </p>
  <br/>
</div>

</stl:block>
