<?xml version="1.0" encoding="utf-8"?>
<stl:block xmlns="http://www.w3.org/1999/xhtml" 
  xmlns:stl="http://xml.itools.org/namespaces/stl">
  <stl:block stl:if="office">
    <h2>${programme/title}</h2>
      <dt stl:repeat="module programme/modules">
          <stl:block stl:if="module/url">
            <a href="${module/url}">${module/title}</a>
          </stl:block>
            <stl:block stl:if="module/exam">
                <stl:block stl:if="not module/exam/passed">
                    Please complete <a
                    href="${module/exam/url}">${module/title} Exam</a>,
                    before you can proceed.
                </stl:block>
                <stl:block stl:if="module/exam/passed">
                    ${module/exam/mark}, ${module/exam/attempts}, Download module
                    certificate
                </stl:block>
            </stl:block>
            <stl:block stl:if="not module/exam">
                NO exam
            </stl:block>
