            <stl:block stl:if="module/marketing">
                <stl:block stl:if="not module/marketing/passed">
                    ${module/marketing/title}
                </stl:block>
                <stl:block stl:if="module/marketing/passed">
                    ${module/marketing/title} Marketing Filled 
                        <stl:block stl:if="module/exam">
                            <stl:block stl:if="not module/exam/passed">
                                Please complete <a
                                href="${module/exam/url}">${module/title} Exam</a>,
                                before you can proceed.
                            </stl:block>
                            <stl:block stl:if="module/exam/passed">
                                ${module/exam/mark}, ${module/exam/attempts}, Download module
                                certificate
                            </stl:block>
                        </stl:block>
                        <stl:block stl:if="not module/exam">
                            No exam
                        </stl:block>
                </stl:block>
            </stl:block>
            -->
            <!-- 
            <stl:block stl:if="module/exam">
                <stl:block stl:if="not module/exam/passed">
                    Please complete <a
                    href="${module/exam/url}">${module/title} Exam</a>,
                    before you can proceed.
                </stl:block>
                <stl:block stl:if="module/exam/passed">
                    ${module/exam/mark}, ${module/exam/attempts}, Download module
                    certificate
                </stl:block>
            </stl:block>
            -->
          <stl:block stl:if="not module/url">${module/title}</stl:block>
          <stl:block stl:if="is_training_manager">
              <h2>Management links</h2>
              <dt>
                <dd>
                  <a href="${module/add_topic}">Add topic</a>
                </dd>
              <stl:block stl:if="module/exam">
                <dd>
                  <a href="${module/exam/url}">Manage exam</a>
                </dd>
              </stl:block>
              <stl:block stl:if="module/marketing">
                <dd>
                   <a href="${module/marketing/url}">Manage marketing form</a>
                </dd>
              </stl:block>
              </dt>
              <stl:block stl:if="not module/exam">
                <dt>
                    <dd>
                      <a href="${module/add_exam}">Add exam</a>
                    </dd>
                </dt>
              </stl:block>
          </stl:block>
      </dt>
  </stl:block>
